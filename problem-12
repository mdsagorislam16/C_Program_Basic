#include <stdio.h>

int main() {
    int n, count;

    printf("Enter the value of N: ");
    scanf("%d", &n);

    printf("Prime numbers from 1 to %d are:\n", n);

    for(int i = 2; i <= n; i++) {  
       count = 0;

      for(int j = 1; j <= i; j++) {
       if(i % j == 0) {
           count++;
            }
        }
        if(count == 2) {
            printf("%d ", i);
            }
    }

    return 0;
}
